FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;//backward
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    div(rhoPhi,U)  Gauss upwind;//Gauss limitedLinearV 1;
    div(phi,alpha)  Gauss vanLeer;
    div(phirb,alpha) Gauss upwind;//Gauss linear;
    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear;
    div(tauDm)          Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear limited 0.33;//Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;//linear;
}

snGradSchemes
{
    default         corrected;//limited 0.33
}


// ************************************************************************* //
